@using Galaxy.Extenstions
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>InstitutionalFinance</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    
    @Styles.Render("~/AdminLTE/plugins/font-awesome/css/font-awesome.min.new.css")
    @Styles.Render("~/AdminLTE/plugins/ionicons/css/ionicons.min.css")
    @Styles.Render("~/AdminLTE/bootstrap/css/bootstrap.min.css")
    @Styles.Render("~/AdminLTE/dist/css/admin-lte.css")
    @Styles.Render("~/AdminLTE/dist/css/skins/skin-blue.min.css")

    @Scripts.Render("~/AdminLTE/plugins/jquery/js/jQuery-2.1.4.min.js")
    @RenderSection("menu", required: false)
    <script src="@Url.Content("~/AdminLTE/bootstrap/js/bootstrap.min.js")"></script>

    @Scripts.Render("~/AdminLTE/plugins/slimscroll/js/jquery.slimscroll.min.js")
    @Scripts.Render("~/Scripts/Shared/_Layout.js")
   
    @Scripts.Render("~/AdminLTE/dist/js/app.js")

</head>
<body class="hold-transition skin-blue sidebar-mini fixed">
    <div class="wrapper">
        <header class="main-header ">
            <a href="@Url.Action("Index", "Product")" class="logo">
                <span class="logo-mini"><b>机构部</b></span>
                <span class="logo-lg"><b>机构金融信息平台</b></span>
            </a>
            <nav class="navbar navbar-static-top" role="navigation">
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <li class="dropdown messages-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-envelope-o"></i>
                                <span class="label label-success">4</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">您有4条未读信息</li>
                                <li>
                                    <ul class="menu">
                                        <li>
                                            <a href="#">
                                                <div class="pull-left">
                                                    <img src="~/AdminLTE/dist/img/user.svg" class="img-circle" alt="User Image">
                                                </div>
                                                <h4>
                                                    管理人A
                                                    <small><i class="fa fa-clock-o"></i> 5 mins</small>
                                                </h4>
                                                <p>产品1净值信息提醒1</p>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <div class="pull-left">
                                                    <img src="~/AdminLTE/dist/img/user.svg" class="img-circle" alt="User Image">
                                                </div>
                                                <h4>
                                                    管理人B
                                                    <small><i class="fa fa-clock-o"></i> 2 hours</small>
                                                </h4>
                                                <p>产品1净值信息提醒1</p>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <div class="pull-left">
                                                    <img src="~/AdminLTE/dist/img/user.svg" class="img-circle" alt="User Image">
                                                </div>
                                                <h4>
                                                    管理人C
                                                    <small><i class="fa fa-clock-o"></i> Today</small>
                                                </h4>
                                                <p>产品1净值信息提醒1</p>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <div class="pull-left">
                                                    <img src="~/AdminLTE/dist/img/user.svg" class="img-circle" alt="User Image">
                                                </div>
                                                <h4>
                                                    管理人D
                                                    <small><i class="fa fa-clock-o"></i> Yesterday</small>
                                                </h4>
                                                <p>产品1净值信息提醒1</p>
                                            </a>
                                        </li>                                     
                                    </ul>
                                </li>
                                <li class="footer"><a href="#">查看全部信息</a></li>
                            </ul>
                        </li>
                      
                        <li class="dropdown tasks-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-flag-o"></i>
                                <span class="label label-danger">9</span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">您有9项代办事宜</li>
                                <li>
                                    <ul class="menu">
                                        <li>
                                            <a href="#">
                                                <h3>
                                                    任务1
                                                    <small class="pull-right">20%</small>
                                                </h3>
                                                <div class="progress xs">
                                                    <div class="progress-bar progress-bar-aqua" style="width: 20%" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">
                                                        <span class="sr-only">20% Complete</span>
                                                    </div>
                                                </div>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <h3>
                                                    任务2
                                                    <small class="pull-right">40%</small>
                                                </h3>
                                                <div class="progress xs">
                                                    <div class="progress-bar progress-bar-green" style="width: 40%" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">
                                                        <span class="sr-only">40% Complete</span>
                                                    </div>
                                                </div>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <h3>
                                                    任务3
                                                    <small class="pull-right">60%</small>
                                                </h3>
                                                <div class="progress xs">
                                                    <div class="progress-bar progress-bar-red" style="width: 60%" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">
                                                        <span class="sr-only">60% Complete</span>
                                                    </div>
                                                </div>
                                            </a>
                                        </li>
                                        <li>
                                            <a href="#">
                                                <h3>
                                                    任务4
                                                    <small class="pull-right">80%</small>
                                                </h3>
                                                <div class="progress xs">
                                                    <div class="progress-bar progress-bar-yellow" style="width: 80%" role="progressbar" aria-valuenow="20" aria-valuemin="0" aria-valuemax="100">
                                                        <span class="sr-only">80% Complete</span>
                                                    </div>
                                                </div>
                                            </a>
                                        </li>
                                    </ul>
                                </li>
                                <li class="footer">
                                    <a href="#">查看所有任务</a>
                                </li>
                            </ul>
                        </li>
                        @*@Html.Partial("_LoginPartial")*@
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-gears"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <aside class="main-sidebar fixed">
            <section class="sidebar">
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/AdminLTE/dist/img/user.svg" class="img-circle" alt="User Image">
                    </div>
                    <div class="pull-left info">
                        <p>@User.Identity.GetUserNameIdentifier()</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> Online</a>
                    </div>
                </div>
                <form action="#" method="get" class="sidebar-form">
                    <div class="input-group">
                        <input type="text" name="q" class="form-control" placeholder="Search...">
                        <span class="input-group-btn">
                            <button type="submit" name="search" id="search-btn" class="btn btn-flat">
                                <i class="fa fa-search"></i>
                            </button>
                        </span>
                    </div>
                </form>
                <ul class="sidebar-menu">
                    <li class="header">主菜单栏</li>
                    
                    <li id="left-sidebar-menu-product">
                        <a href="@Url.Action("Index", "Product")">
                            <i class="fa fa-th"></i> <span>产品信息</span>
                            <small class="label pull-right bg-green">4</small>
                        </a>
                    </li>
                    <li id="left-sidebar-menu-widgets">
                        <a href="@Url.Action("Index", "Widgets")">
                            <i class="fa fa-laptop"></i> <span>投顾信息</span>
                            <small class="label pull-right bg-red">NEW</small>
                        </a>
                    </li>
                    <li id="left-sidebar-menu-charts" class="treeview">
                        <a href="#">
                            <i class="fa fa-pie-chart"></i>
                            <span>业绩分析</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="left-sidebar-menu-charts-chartjs"><a href="@Url.Action("ChartJs", "Charts")"><i class="fa fa-circle-o"></i>收益率分析</a></li>
                            <li id="left-sidebar-menu-charts-morris"><a href="@Url.Action("Morris", "Charts")"><i class="fa fa-circle-o"></i> ALPHA策略分析</a></li>
                            <li id="left-sidebar-menu-charts-flot"><a href="@Url.Action("Flot", "Charts")"><i class="fa fa-circle-o"></i> CTA策略分析</a></li>
                            <li id="left-sidebar-menu-charts2" class="treeview">
                                <a href="#">
                                    <i class="fa fa-edit"></i>
                                    <span>投资组合</span>
                                    <i class="fa fa-angle-left pull-right"></i>
                                </a>
                                <ul class="treeview-menu">
                                    <li id="left-sidebar-menu-charts-chartjs"><a href="@Url.Action("ChartJs", "Charts")"><i class="fa fa-circle-o text-blue"></i>产品相关系数</a></li>
                                    <li id="left-sidebar-menu-charts-morris"><a href="@Url.Action("Morris", "Charts")"><i class="fa fa-circle-o text-maroon"></i> 策略相关系数</a></li>
                                 
                                </ul>
                            </li>
                        </ul>
                    </li>
                    <li id="left-sidebar-menu-elements" class="treeview">
                        <a href="#">
                            <i class="fa fa-laptop"></i>
                            <span>报表系统</span>
                            <i class="fa fa-angle-left pull-right"></i>
                        </a>
                        <ul class="treeview-menu">
                            <li id="left-sidebar-menu-elements-general"><a href="@Url.Action("General", "Elements")"><i class="fa fa-circle-o"></i> 产品日终报表</a></li>
                            <li id="left-sidebar-menu-elements-icons"><a href="@Url.Action("Icons", "Elements")"><i class="fa fa-circle-o"></i> 产品收益率表</a></li>
                            <li id="left-sidebar-menu-elements-buttons"><a href="@Url.Action("Buttons", "Elements")"><i class="fa fa-circle-o"></i> 产品持仓表</a></li>
                            <li id="left-sidebar-menu-elements-sliders"><a href="@Url.Action("Sliders", "Elements")"><i class="fa fa-circle-o"></i> 投顾信息表</a></li>
                        </ul>
                    </li>
                    <li class="header">常用菜单</li>
                    <li><a href="#"><i class="fa fa-circle-o text-red"></i> <span>菜单1</span></a></li>
                    <li><a href="#"><i class="fa fa-circle-o text-yellow"></i> <span>菜单2</span></a></li>
                    <li><a href="#"><i class="fa fa-circle-o text-aqua"></i> <span>菜单3</span></a></li>
                </ul>
            </section>
        </aside>
        <div class="content-wrapper">
            <section class="content-header">
                <h1>
                    @ViewBag.Title
                    <small>@ViewBag.SubTitle</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-dashboard"></i>@ViewBag.Title</a></li>
                    @*<li class="active">@ViewBag.SubTitle</li>*@
                </ol>
            </section>
            <section class="content">
                @RenderBody()
            </section>
        </div>
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 2.3.2
            </div>
            <strong>Copyrights | 中信建投证券股份有限公司| &copy; 2017-2020 <a href="#">机构金融部</a>.</strong> All rights
            reserved.
        </footer>
        <aside class="control-sidebar control-sidebar-dark">
            <ul class="nav nav-tabs nav-justified control-sidebar-tabs">
                <li><a href="#control-sidebar-settings-tab" data-toggle="tab"><i class="fa fa-gears"></i></a></li>
            </ul>
            <div class="tab-content">
                <form method="post">
                    <h3 class="control-sidebar-heading">信息提醒设置</h3>
                    <div class="form-group">
                        <label class="control-sidebar-subheading">
                            产品1信息提醒设置
                            <input type="checkbox" class="pull-right" checked>
                        </label>
                        <p>
                            产品1净值跌破0.8，提示信息接收
                        </p>
                    </div>
                    <div class="form-group">
                        <label class="control-sidebar-subheading">
                            产品2信息提醒设置
                            <input type="checkbox" class="pull-right" checked>
                        </label>
                        <p>
                            产品2净值跌破0.7，提示信息接收
                        </p>
                    </div>
                    <div class="form-group">
                        <label class="control-sidebar-subheading">
                            产品3信息提醒设置
                            <input type="checkbox" class="pull-right" checked>
                        </label>
                        <p>
                            产品3净值跌破0.7，提示信息接收
                        </p>
                    </div>
                   
                </form>
            </div>
        </aside>
        <div class="control-sidebar-bg"></div>
    </div>


</body>
@RenderSection("scripts", required: false)
</html>
