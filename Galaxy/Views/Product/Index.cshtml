@{
    ViewBag.Title = "产品信息";
    ViewBag.SubTitle = "产品列表";
}

@section Menu {
    @Scripts.Render("~/Scripts/Product/menu.js")
}

@Styles.Render("~/AdminLTE/plugins/datatables/css")

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
            </div>
            <div class="box-body">
                <table id="productListTable" class="table table-bordered table-striped table-hover table-condensed">
                    <thead>
                        <tr>
                            <th>产品名称</th>
                            <th>成立日期</th>
                            <th>托管银行</th>
                            <th>投资顾问</th>
                            <th>管理人</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Trident</td>
                            <td>
                                Internet
                                Explorer 4.0
                            </td>
                            <td>Win 95+</td>
                            <td> 4</td>
                            <td>X</td>
                            <td>@Html.ActionLink("详细信息","Detail")</td>
                        </tr>
                        <tr>
                            <td>Trident</td>
                            <td>
                                Internet
                                Explorer 5.0
                            </td>
                            <td>Win 95+</td>
                            <td>5</td>
                            <td>C</td>
                            <td>@Html.ActionLink("详细信息", "Detail")</td>
                        </tr>
                        <tr>
                            <td>Trident</td>
                            <td>
                                Internet
                                Explorer 5.5
                            </td>
                            <td>Win 95+</td>
                            <td>5.5</td>
                            <td>A</td>
                            <td>@Html.ActionLink("详细信息", "Detail")</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @Scripts.Render("~/AdminLTE/plugins/datatables/js")
    @Scripts.Render("~/Scripts/Product/productData.js")
}

